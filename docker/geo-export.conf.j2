include "application"

com.socrata {
  geoexport {
    threadpool {
      min-threads = {{ MIN_THREADS }}
      max-threads = {{ MAX_THREADS }}
      idle-timeout = 1200000
    }
  }
  common-zk-ensemble = {{ ZOOKEEPER_ENSEMBLE }}

  curator {
    ensemble = {{ ZOOKEEPER_ENSEMBLE }}
    address = "{{ ARK_HOST }}"
  }
  network {
    port = 7777
  }
}
